{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "plotly.js"
    ],
    "paths": {
      "plotly.js": [
        "./node_modules/plotly.js/lib/core.js",
        "./node_modules/plotly.js/lib/scatter.js"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
